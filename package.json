{
  "name": "typeripper",
  "version": "0.1.0",
  "description": "Sound type inference for JavaScript at the AST level",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "javascript",
    "type-inference",
    "static-analysis",
    "ast",
    "control-flow"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
